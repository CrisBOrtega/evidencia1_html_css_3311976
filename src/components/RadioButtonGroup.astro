---
    interface Props {
       etiqueta: string;
    }
        
    const  etiqueta  = Astro.props.etiqueta;
---
<label>{etiqueta}</label>
<div>
    <div id="cedula">
        <input 
            type="radio"
            name="rbg"
            id="radio1" 
        /> 
        <strong> CC </strong>
    </div>
    <div id="tarjeta">
        <input 
            type="radio" 
            name="rbg"
            id="radio2" 
        />
        <strong>TI</strong>
    </div>
    
    <input 
        type="radio" 
        name="rbg"
        id="radio3"
    />CE   
</div>
<style>
    #cedula{
        background-color:brown;
        display: inline;
    }
    #tarjeta{
        background-color:cyan;
        display: inline;
    }
</style>